		"@playwright/test": "^1.51.1",
		"@self/build": "workspace:^",
		"@self/tootils": "workspace:^",
		"@sveltejs/package": "^2.3.4",
		"@sveltejs/vite-plugin-svelte": "^3.1.0",
		"@tailwindcss/forms": "^0.5.7",
		"@testing-library/dom": "^10.1.0",
