#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/peterallen/Projects/gradio/ui/node_modules/.pnpm/cssesc@3.0.0/node_modules/cssesc/bin/node_modules:/Users/peterallen/Projects/gradio/ui/node_modules/.pnpm/cssesc@3.0.0/node_modules/cssesc/node_modules:/Users/peterallen/Projects/gradio/ui/node_modules/.pnpm/cssesc@3.0.0/node_modules:/Users/peterallen/Projects/gradio/ui/node_modules/.pnpm/node_modules:/Users/peterallen/Projects/gradio/ui/node_modules:/Users/peterallen/Projects/gradio/node_modules:/Users/peterallen/Projects/node_modules:/Users/peterallen/node_modules:/Users/node_modules:/node_modules:/Users/peterallen/Projects/gradio/ui/node_modules/.pnpm/node_modules/cssesc/bin/node_modules:/Users/peterallen/Projects/gradio/ui/node_modules/.pnpm/node_modules/cssesc/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/peterallen/Projects/gradio/ui/node_modules/.pnpm/cssesc@3.0.0/node_modules/cssesc/bin/node_modules:/Users/peterallen/Projects/gradio/ui/node_modules/.pnpm/cssesc@3.0.0/node_modules/cssesc/node_modules:/Users/peterallen/Projects/gradio/ui/node_modules/.pnpm/cssesc@3.0.0/node_modules:/Users/peterallen/Projects/gradio/ui/node_modules/.pnpm/node_modules:/Users/peterallen/Projects/gradio/ui/node_modules:/Users/peterallen/Projects/gradio/node_modules:/Users/peterallen/Projects/node_modules:/Users/peterallen/node_modules:/Users/node_modules:/node_modules:/Users/peterallen/Projects/gradio/ui/node_modules/.pnpm/node_modules/cssesc/bin/node_modules:/Users/peterallen/Projects/gradio/ui/node_modules/.pnpm/node_modules/cssesc/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../cssesc/bin/cssesc" "$@"
else
  exec node  "$basedir/../cssesc/bin/cssesc" "$@"
fi
